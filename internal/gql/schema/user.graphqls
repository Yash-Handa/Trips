type User {
  id: ID!
  email: String!
  firstName: String!
  lastName: String
  createdAt: Time!

  trips: [Trip!]!
}

input RegisterInput {
  email: String!
  password: String!
  confirmPassword: String!
  firstName: String!
  lastName: String
}

input LoginInput {
  email: String!
  password: String!
}

type AuthToken {
  accessToken: String!
  expiredAt: Time!
}

type AuthResponse {
  authToken: AuthToken!
  user: User!
}